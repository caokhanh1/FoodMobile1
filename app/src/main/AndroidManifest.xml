            android:exported="false" />
        <activity android:name=".Activity.Activity.UserProfileActivity" />
        <activity
            android:name=".Activity.Activity.CartActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.Activity.ListFoodFavoriteActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.Activity.ListFoodActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.Activity.DetailActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.Activity.MainActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.Activity.IntroActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.Activity.SignupActivity" />
        <activity android:name=".Activity.Activity.LoginActivity" />
    </application>

</manifest>